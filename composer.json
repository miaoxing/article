{
  "name": "miaoxing/article",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Article Plugin",
  "keywords": [
    "miaoxing",
    "article"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/app": "^0.3.3",
    "miaoxing/link-to": "^0.1.17"
  },
  "require-dev": {
    "miaoxing/dev": "^7.0.1"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Article\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Article\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/plugin": "miaoxing/plugin as 0.4.x-dev",
      "miaoxing/services": "miaoxing/services as 0.3.x-dev"
    }
  }
}
