{
  "name": "miaoxing/article",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Article Plugin",
  "keywords": [
    "miaoxing",
    "article"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/admin": "^0.11.2",
    "miaoxing/app": "^0.7.8",
    "miaoxing/link-to": "^0.1.44"
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.3"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Article\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Article\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/plugin": "miaoxing/plugin as 0.13.x-dev",
      "miaoxing/services": "miaoxing/services as 0.8.x-dev",
      "miaoxing/admin": "miaoxing/admin as 0.11.x-dev",
      "miaoxing/app": "miaoxing/app as 0.7.x-dev",
      "wei/wei": "twinh/wei as 0.16.x-dev"
    }
  }
}
