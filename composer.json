{
  "name": "miaoxing/article",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Article Plugin",
  "keywords": [
    "miaoxing",
    "article"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/admin": "^0.5.1",
    "miaoxing/app": "^0.5.1",
    "miaoxing/link-to": "^0.1.20"
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Article\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Article\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/plugin": "miaoxing/plugin as 0.7.x-dev",
      "miaoxing/services": "miaoxing/services as 0.4.x-dev",
      "miaoxing/admin": "miaoxing/admin as 0.5.x-dev",
      "wei/wei": "twinh/wei as 0.11.x-dev"
    }
  }
}
